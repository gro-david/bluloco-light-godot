	"tokenColors": [
		{
			"name": "Keyword",
			"scope": ["keyword.language.gdscript", "keyword.control.gdscript", "keyword.operator.wordlike.gdscript"],
			"settings": {
				"foreground": "#43AFE0",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Inherited Class",
			"scope": "entity.other.inherited-class.gdscript",
			"settings": {
				"foreground": "#D52753",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Class",
			"scope": "entity.name.type.class.gdscript",
			"settings": {
				"foreground": "#D52753",
				"fontStyle": "bold"
			}
		},
    {
      "name": "Support Class",
      "scope": "support.class.library.gdscript",
      "settings": {
        "foreground": "#D52753"
      }
    },
		{
			"name": "Var",
			"scope": "storage.type.var.gdscript",
			"settings": {
				"foreground": "#43AFE0"
			}
		},
		{
			"name": "Operator",
			"scope": ["keyword.operator.gdscript", "keyword.operator.arithmetic.gdscript"],
			"settings": {
				"foreground": "#7A82DA",
			}
		},
		{
			"name": "Numbers",
			"scope": ["constant.numeric.integer.gdscript", "constant.numeric.float.gdscript"],
			"settings": {
				"foreground": "#CE35C0",
			}
		},
		{
			"name": "Func, Signal",
			"scope": "storage.type.function.gdscript",
			"settings": {
				"foreground": "#43AFE0",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Function Name, Signal Name",
			"scope": "entity.name.function.gdscript",
			"settings": {
				"foreground": "#23974A",
			}
		},
		{
			"name": "Comment",
			"scope": "comment.line.number-sign.gdscript",
			"settings": {
				"foreground": "#A0A1A7",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Function Call",
			"scope": "meta.function-call.gdscript",
			"settings": {
				"foreground": "#23974A",
			}
		},
		{
			"name": "Nodepaths",
			"scope": "meta.literal.nodepath.gdscript",
			"settings": {
				"foreground": "#C5A332",
			}
		},
		{
			"name": "Operator",
			"scope": ["punctuation.separator.annotation.gdscript", "keyword.operator.assignment.gdscript"],
			"settings": {
				"foreground": "#7A82DA",
			}
		},
		{
			"name": "String",
			"scope": ["string.quoted.double.gdscript", "string.quoted.single.gdscript"],
			"settings": {
				"foreground": "#C5A332",
			}
		},
		{
			"name": "Constants",
			"scope": "constant.language.gdscript",
			"settings": {
        "foreground": "#A05A48",
			}
		},
		{
			"name": ".new()",
			"scope": "storage.type.new.gdscript",
			"settings": {
				"foreground": "#43AFE0",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Decorators",
			"scope": ["entity.name.function.decorator.gdscript", "keyword.control.flow"],
			"settings": {
				"foreground": "#CC5E00",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Shorthand Node Selection",
			"scope": "support.function.builtin.shorthand.gdscript",
			"settings": {
				"foreground": "#D52753",
				"fontStyle": "bold"
			},
		},
		{
			"name": "Invalid",
			"scope": "invalid.illegal.escape.gdscript",
			"settings": {
				"fontStyle": "strikethrough",
				"foreground": "#a90000"
			}
		},
		{
			"name": "Brackets",
			"scope": ["punctuation.definition.parameters.begin.gdscript",
        "punctuation.definition.parameters.end.gdscript",
        "punctuation.definition.arguments.begin.gdscript",
        "punctuation.definition.arguments.end.gdscript"],
			"settings": {
				"foreground": "#23974A",
			}
